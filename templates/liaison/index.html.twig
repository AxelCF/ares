{% extends 'base.html.twig' %}

{% block title %}Liaison index
{% endblock %}

{% block body %}
	<h1>Liaison index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>professor</th>
				<th>subject</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for liaison in liaisons %}
				<tr>
					<td>{{ liaison.id }}</td>
					<td>{{ liaison.getProfessor }}</td>
					<td>{{ liaison.getSubject }}</td>
					<td>
						<a href="{{ path('app_liaison_show', {'id': liaison.id}) }}">show</a>
						<a href="{{ path('app_liaison_edit', {'id': liaison.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="2">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_liaison_new') }}">Create new</a>
{% endblock %}
